{
  "name": "state-router-svelte-todo",
  "version": "1.0.0",
  "private": false,
  "description": "Todo-App with abstract-state-router, Svelte and TypeScript",
  "scripts": {
    "build": "webpack && copyfiles ./index.html ./dist",
    "build:min": "SET NODE_ENV=production&& npm run build",
